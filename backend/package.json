{
  "name": "shadow-coach-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Shadow-Self Coach mobile app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:status": "sequelize-cli db:migrate:status",
    "migrate:create": "sequelize-cli migration:generate --name",
    "seed": "node scripts/seed.js",
    "db:sync": "node scripts/sync-database.js",
    "db:create-tables": "node scripts/create-tables.js",
    "db:create-mood-table": "node scripts/create-mood-table.js"
  },
  "keywords": [
    "ai",
    "coaching",
    "mental-health",
    "therapy"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "sequelize": "^6.35.0",
    "openai": "^4.20.0",
    "axios": "^1.6.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "crypto": "^1.0.1",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "sequelize-cli": "^6.6.2",
    "ws": "^8.16.0",
    "@pinecone-database/pinecone": "^1.1.2",
    "weaviate-ts-client": "^1.5.0",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "@types/node": "^20.10.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}

